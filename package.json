{
  "name": "@cuppachino/ts-node-next",
  "version": "1.0.0",
  "description": "ðŸ”¥ Blazing TS NodeNext (esm) template with ts-node & SWC",
  "keywords": [
    "esm",
    "NodeNext",
    "ts-node",
    "swc",
    "typescript",
    "template",
    "restart"
  ],
  "author": "cuppachino <cuppachino.dev@gmail.com>",
  "license": "GPL-3.0",
  "files": [
    "dist"
  ],
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node-esm ./src/index.ts",
    "build:swc": "ts-node ./src/index.ts",
    "build:tsc": "tsc --build --clean && tsc",
    "build:clean": "tsc --build --clean",
    "build:start": "node ./dist/index.js"
  },
  "devDependencies": {
    "@swc/core": "^1.3.26",
    "@swc/helpers": "^0.4.14",
    "@types/node": "^18.11.18",
    "nodemon": "^2.0.20",
    "regenerator-runtime": "^0.13.11",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {}
}
